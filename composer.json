{
    "name": "ins/media-api",
    "type": "project",
    "description": "Example for integration of Sonata MediaBundle with ApiPlatform 2.0 including media upload and exposing media formats in GET item operation",
    "require": {
        "sonata-project/media-bundle": "3.10.0",
        "cweagans/composer-patches": "^1.5",
        "liip/imagine-bundle": "^1.6",
        "sensio/buzz-bundle": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Ins\\MediaApiBundle\\": ""
        }
    },
    "extra": {
        "patches": {
            "sonata-project/media-bundle": {
                "Workaround for UUID to perform math operation": "https://bitbucket.org/gotoemma/media-api/raw/master/Resources/patches/0001-E2-29-Workaround-for-UUID-to-perform-math-operation.patch"
            }
        }
    }
}
